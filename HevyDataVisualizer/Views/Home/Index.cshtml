@{
    ViewData["Title"] = "Home Page";
}

@section Scripts {
    <script src="~/js/views/home.js"></script>
}

<div class="text-center">
    <h1 class="display-4">Welcome, please upload your .csv file!</h1>
    <form id="formWorkout" method="post" enctype="multipart/form-data">
        <div class="mb-3">
            <label for="formFile" class="form-label">Default file input example</label>
            <input class="form-control" type="file" id="fileWorkout" accept=".csv">
        </div>
        <button class="btn btn-primary">Upload File</button>
    </form>

</div>